<h1>Listing preferred_names</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @preferred_names.each do |preferred_name| %>
  <tr>
    <td><%= preferred_name.name %></td>
    <td><%= link_to 'Show', preferred_name %></td>
    <td><%= link_to 'Edit', edit_preferred_name_path(preferred_name) %></td>
    <td><%= link_to 'Destroy', preferred_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

