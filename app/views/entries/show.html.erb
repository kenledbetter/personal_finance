<p id="notice"><%= notice %></p>

<p>
  <b>Date:</b>
  <%= @entry.date %>
</p>
<p>
  <b>Raw name:</b>
  <%= @entry.raw_name %>
</p>
<p>
  <b>Final name:</b>
  <%= @entry.final_name.try(:name) %>
</p>
<p>
  <b>Amount:</b>
  <%= @entry.amount %>
</p>
<p>
  <b>Account:</b>
  <%= @entry.account.try(:name) %>
</p>

<%= link_to 'Match', match_entry_path(@entry) %> |
<%= link_to 'Edit', edit_entry_path(@entry) %> |
<%= link_to 'Back', entries_path %>
